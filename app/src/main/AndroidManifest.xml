<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.guiadasavesparaibanas">

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory">
        <activity android:name=".DetalheAveActivity"></activity>
        <activity android:name=".visao.ReferenciasActivity" />
        <activity android:name=".visao.SobreActivity" />
        <activity
            android:name=".visao.OrnitologiaActivity"
            android:parentActivityName=".visao.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".visao.MainActivity" />
        </activity>
        <activity
            android:name=".visao.ImportanciaActivity"
            android:parentActivityName=".visao.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".visao.MainActivity" />
        </activity>
        <activity
            android:name=".visao.ListaActivity"
            android:parentActivityName=".visao.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".visao.MainActivity" />
        </activity>
        <activity
            android:name=".visao.ProblematicaActivity"
            android:parentActivityName=".visao.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".visao.MainActivity" />
        </activity>
        <activity android:name=".visao.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>